<div class="auth-body">
  <label class="fof-title">FREE OR FOR SALE</label>
  <br>
  <label class='for-university-students'>For University Students</label>
  <br>

    <div class="guest-sign-in-laurie">
      <form action="<%= session_url %>" method="post" class="auth_token">

        <input
        type="hidden"
        name="authenticity_token"
        id=""
        value="<%= form_token %>">


        <input
        type="hidden"
        name="user[username]"
        id="user_username"
        value="laurieou">


        <input
        type="hidden"
        name="user[password]"
        id="user_password"
        value="laurie">

        <input type="submit"  class="guest-sign-in-laurie-button" value="Demo Sign In as Laurie">
      </form>
    </div>

<div class="auth-container-sign-in">

    <div class="fofs-title">
    </div>

      <div class='form-input'>
        <h1 class='whitetext'>Sign In</h1>

        <form class="form-group" action="<%= session_url %>" method="post">
          <%= form_token %>


          <label class='whitetext'>Username</label> 
          <label class='whitetext' for="Username">Username</label>
          <input type="text" name="user[username]">
          <br>
          <br>


          <label class='whitetext' for="user_password">Password</label>
          <input type="password" name="user[password]">
          <br>
          <br>

          <input type="submit" value="Sign In">
        </form>
    </div>


    <div class="whitetext">
      New User?
      <a href="/users/new" class="link">Sign Up here!</a>
    </div>

  </div>
</div>
